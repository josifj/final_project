<div class="page-header">
  <h1>
    Users
    <a href="/users/new" class="btn btn-lg btn-success">New User</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Email</th>
        <th>Password</th>
        <th>Name</th>
        <th>Graduation year</th>
        <th>Admin</th>
        <th>Phone number</th>
        <th>Address</th>
        <th>Building</th>
        <th>Apt no</th>
        <th>Cal</th>
        <th>Actions</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
        <td><%= user.name %></td>
        <td><%= user.graduation_year %></td>
        <td><%= user.admin %></td>
        <td><%= user.phone_number %></td>
        <td><%= user.address %></td>
        <td><%= user.building.id %></td>
        <td><%= user.apt_no %></td>
        <td><%= user.cal_id %></td>
        <td>
          <a href="/users/<%= user.id %>" class="btn btn-primary">Show</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
